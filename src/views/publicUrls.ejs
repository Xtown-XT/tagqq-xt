<!DOCTYPE html>
<html>
<head>
  <meta charset="UTF-8">
  <title>Public URL Report</title>
  <style>
    body { font-family: Arial, sans-serif; }
    h1 { text-align: center; }
    table { width: 100%; border-collapse: collapse; margin-top: 20px; }
    th, td { border: 1px solid #000; padding: 8px; text-align: left; }
    th { background-color: #f0f0f0; }
  </style>
</head>
<body>
  <h1>Public URL Report</h1>
  <p><strong>Date:</strong> <%= date %></p>
  <p><strong>Total Count:</strong> <%= totalCount %></p>

  <table>
    <thead>
      <tr>
        <th>#</th>
        <th>ID</th>
        <th>Status</th>
        <th>User ID</th>
        <th>Order ID</th>
        <th>Username</th>
        <th>Email</th>
        <th>Phone</th>
      </tr>
    </thead>
    <tbody>
      <% urls.forEach(url => { %>
        <tr>
          <td><%= url.index %></td>
          <td><%= url.id %></td>
          <td><%= url.status %></td>
          <td><%= url.user_id %></td>
          <td><%= url.order_id %></td>
          <td><%= url.username %></td>
          <td><%= url.email %></td>
          <td><%= url.phone %></td>
        </tr>
      <% }) %>
    </tbody>
  </table>


</body>
</html>
